/* components/musics/musics.wxss */
.container{
  display: flex;
  flex-direction: column;
}
.cover{
  width:212rpx;
  height:212rpx;
  margin-bottom: 10rpx;
}
.name{
  width: 212rpx;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  align-content: center;
  margin-bottom: 30rpx;
}
.singer{
  font-size: 20rpx;
  color:#888888;
}